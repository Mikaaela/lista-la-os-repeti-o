#include <stdio.h>

int main() {
    int a;
    int count_negativos = 0;
    
    for (int i = 0; i < 5; i++) {
        printf("Digite o valor de a: ");
        scanf("%d", &a);
        
        if (a < 0) {
            count_negativos++;
        }
    }
    
    printf("Quantidade de valores negativos: %d
", count_negativos);
    
    return 0;
}